<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Restaurant.SignupController">
   <children>
      <Label layoutX="325.0" layoutY="30.0" text="Name" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="325.0" layoutY="350.0" text="Password" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="415.0" layoutY="30.0" promptText="***" style="-fx-background-color: #fae6f2;" />
      <PasswordField fx:id="password" layoutX="415.0" layoutY="350.0" style="-fx-background-color: #fae6f2;" />
      <ButtonBar layoutX="365.0" layoutY="386.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetAction" style="-fx-background-color: #D70F64;" text="Reset">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="signupButton" mnemonicParsing="false" onAction="#signupAction" style="-fx-background-color: #D70F64;" text="Sign Up">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </buttons>
      </ButtonBar>
      <AnchorPane layoutY="16.0" prefHeight="378.0" prefWidth="238.0" style="-fx-background-color: #D70F64;">
